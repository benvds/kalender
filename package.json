{
  "name": "kalender",
  "version": "0.4.1",
  "description": "Basic component for rendering calendars.",
  "main": "./src/kalender.js",
  "dependencies": {},
  "devDependencies": {
    "mocha": "^2.2.1",
    "ts-node": "^0.9.3",
    "tsd": "^0.6.5",
    "tsify": "^0.16.0",
    "typescript": "^1.8.10",
    "uglify-js": "^2.4.20"
  },
  "scripts": {
    "test": "mocha --compilers ts:ts-node/register",
    "test:watch": "mocha --watch test --compilers ts:ts-node/register",
    "build": "browserify ./src/kalender.ts --plugin tsify --standalone kalender --debug | exorcist ./dist/kalender.js.map > ./dist/kalender.js",
    "compress": "uglifyjs --compress warnings=false --mangle --bare-returns --output ./dist/kalender.min.js -- ./dist/kalender.js",
    "dist": "npm run build && npm run compress"
  },
  "author": {
    "name": "Ben van de Sande",
    "email": "ben@pomodoro.nl"
  },
  "license": "MIT"
}
